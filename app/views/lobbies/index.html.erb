<h1>Lobbies</h1>

<p><%= flash[:error] %></p>
<p><%= flash[:success] %></p>

<% if !in_lobby? %>
  <%= button_to "Create a new lobby", new_lobby_path, :method => "get", class: "btn btn-primary"  %>
<% end %> <br>

<div>
  <%= render partial: "lobbies", locals: { lobbies: @lobbies, lobbies_page: true } %>
</div>
